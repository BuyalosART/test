{
  "name": "gui",
  "version": "1.0.0",
  "description": "",
  "module": "build/gui.module.js",
  "scripts": {
    "lint": "eslint . --fix",
    "icons": "node style/iconfont.js",
    "css": "sass style/main.scss build/gui.css --style=compressed --no-charset --no-source-map",
    "js": "rollup -c",
    "dev:icons": "onchange 'style/icons/*.svg' -- npm run icons",
    "dev:css": "npm run css -- --watch",
    "dev:js": "npm run js -- -w",
    "dev:server": "live-server --watch=build,demo --open=demo --wait=20",
    "dev": "concurrently npm:dev:*",
    "build": "npm run icons && npm run css && npm run js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.1.1",
    "eslint": "^6.1.0",
    "live-server": "^1.2.1",
    "onchange": "^6.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-string": "^3.0.0",
    "sass": "^1.22.10",
    "webfont": "^9.0.0"
  },
  "eslintIgnore": [
    "build"
  ]
}
